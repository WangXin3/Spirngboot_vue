<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.wxx.springbootvue.mapper.PermitMapper">


    <select id="getPermitListById" resultType="com.wxx.springbootvue.dao.Permission">
        SELECT *
        FROM tb_permission tp
        WHERE tp.parent_id = #{id};
    </select>
</mapper>